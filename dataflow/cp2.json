{
	"name": "cp2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RawClientes",
						"type": "DatasetReference"
					},
					"name": "dbcli"
				},
				{
					"dataset": {
						"referenceName": "RawClientes",
						"type": "DatasetReference"
					},
					"name": "dfclicf"
				},
				{
					"dataset": {
						"referenceName": "RawClientes",
						"type": "DatasetReference"
					},
					"name": "dbend"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "RawClientes",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "join2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          NR_CLIENTE as short,",
				"          NM_CLIENTE as string,",
				"          QT_ESTRELAS as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dbcli",
				"source(output(",
				"          NR_CLIENTE as short,",
				"          FL_SEXO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dfclicf",
				"source(output(",
				"          NR_CLIENTE as short,",
				"          CD_LOGRADOURO_CLI as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dbend",
				"dbcli, dfclicf join(dbcli@NR_CLIENTE == dfclicf@NR_CLIENTE,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1, dbend join(dbcli@NR_CLIENTE == dbend@NR_CLIENTE,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}